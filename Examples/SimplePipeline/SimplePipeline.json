{
    "source1": {
        "type": "icvThreadedNode",
        "function": { 
            "type": "icvRandomSource",
            "interval": "100"
        }
    },
    "source2": {
        "type": "icvThreadedNode",
        "function": { 
            "type": "icvRandomSource",
            "interval": "200"
        }
    },
    "filter1": {
        "type": "icvThreadedNode",
        "function": {
            "type": "PolynomialFunction",
            "coefficients": [1, 2, 3]
        },
        "input": "source1"
    },
    "filter2": {
        "type": "icvThreadedNode",
        "function": "AddFunction",
        "input": ["filter1", "source2"]
    },
    "sink": {
        "type": "icvThreadedNode",
        "function": "icvStdStreamSink",
        "input": "filter2"
    }
}